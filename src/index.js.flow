function f<F, B>(...folders: F) {
  declare function d<T, B>(Base: B): $Call<
    & ((X: Class<typeof Component>) => Class<React$Component<T>>)
    & ((X: Class<typeof PureComponent>) => Class<React$Component<T>>)
    & ((any) => Class<React$Component<{
        ...$Exact<React$ElementConfig<B>>,
        ...$Exact<T>,
    }>>)
  , B>

  return d
}


export default f
